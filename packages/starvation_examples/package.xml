<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>starvation_examples</name>
  <version>1.0.0</version>
  <description>Examples for starvation in the multi-threaded executor in ROS 2. Supplementary material for the paper "Thread Carefully: Preventing Starvation in the
ROS 2 Multithreaded Executor" by Teper et al. at EMSOFT 2024.</description>
  <maintainer email="harun.teper@tu-dortmund.de">Harun Teper</maintainer>
  <license>MIT</license>

  <buildtool_depend>ament_cmake</buildtool_depend>
  <buildtool_depend>ament_cmake_python</buildtool_depend>

  <depend>rclcpp</depend>
  <depend>rclpy</depend>

  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>
  
  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
